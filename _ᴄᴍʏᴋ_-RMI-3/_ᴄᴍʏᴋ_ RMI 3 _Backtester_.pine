plot                        ( VIEWPOSITIONS and LONG_ALERT[1]       ? (close*2) : LONG_ALERT[1]     ? 50 : 0        , color=C5A         , title="OPEN 1 LONG")

plot                        ( VIEWPOSITIONS and SHORT_ALERT[1]      ? (close*2) : SHORT_ALERT[1]    ? 50 : 0        , color=Y6A         , title="OPEN 1 SHORT")

plot                        ( VIEWPOSITIONS and CLOSE_ONE           ? (close*2) : CLOSE_ONE         ? 50 : 0        , color=M4A         , title="SL/TP 1 POSITION" )

plot                        ( VIEWPOSITIONS and CLOSE_ALERT         ? (close*2) : CLOSE_ALERT       ? 50 : 0        , color=white       , title="CLOSE POSITIONS"  )


